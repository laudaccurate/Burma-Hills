                        i.now-ui-icons.location_pin
                      a.btn.btn-success.btn-round.btn-icon.btn-icon-mini.btn-neutral(href='#', data-toggle='tooltip', data-placement='bottom' title='Confirm Attendance', data-original-title='Confirm Attendance')
                        i.now-ui-icons.ui-2_like
